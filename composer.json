{
  "name": "farpat/api",
  "description": "API Client in PHP",
  "version": "1.1.2",
  "type": "library",
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "symfony/var-dumper": "^5.0"
  },
  "authors": [
    {
      "name": "Patrick FARRUGIA",
      "email": "p.farrugia@outlook.fr"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Farpat\\Api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "phpunit --testdox"
  }
}
